<% include ./partials/headercontact %>



        <div class="container">
          <div class="py-5 text-center">
            
            <h2>Contact Us</h2>
            <p class="lead">If you have any questions drop us a line.</p>
          </div>
    
    
          
            <div class="">
              <h4 class="mb-3"></h4>
              <form method="POST" class="needs-validation was-validated" action="https://liveformhq.com/form/7fe18e47-be52-4413-9fa5-23e2fc132b62">
                <div class="row">
                  <div class="col-md-6 mb-3">
                    <label for="firstName">First name</label>
                    <input name="name" type="text" class="form-control" id="firstName" placeholder="" value="" required="">
                    <div class="invalid-feedback">
                      Valid first name is required.
                    </div>
                  </div>
                  <div class="col-md-6 mb-3">
                    <label for="lastName">Last name</label>
                    <input name="last name" type="text" class="form-control" id="lastName" placeholder="" value="" required="">
                    <div class="invalid-feedback">
                      Valid last name is required.
                    </div>
                  </div>
                </div>
    
               
    
                <div class="mb-3">
                  <label for="email">Email <span class="text-muted">(Optional)</span></label>
                  <input name="email" type="email" class="form-control" id="email" placeholder="you@example.com">
                  <div class="invalid-feedback">
                    Please enter a valid email address for shipping updates.
                  </div>
                </div>
    
                <div class="mb-3">
                  <label for="message">Message</label>
                  <input name="message" type="text" class="form-control" id="address" placeholder="" required="">
                  <div class="invalid-feedback">
                    Please enter your message
                  </div>
                </div>
                <input type="hidden" name="_redirect" value="https://alpacadamia-1.herokuapp.com/thankyou">
               
                <hr class="mb-4">
                <button id="myBtn" class="btn btn-primary btn-lg " type="submit">Submit</button>
                <p id="demo"></p>

           

              </form>
            </div>
          </div>
    
          
    
